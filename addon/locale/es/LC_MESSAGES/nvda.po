msgstr "Descargado: {progress}%"

#. Translators: message shown in the voice download progress dialog
#: addon\globalPlugins\piper_voices_globalPlugin\voice_download.py:177
msgid "Installing voice"
msgstr "Instalando voz"

#: addon\globalPlugins\piper_voices_globalPlugin\voice_download.py:206
#, python-brace-format
msgid ""
"Successfully downloaded voice  {voice}.\n"
"To use this voice, you need to restart NVDA.\n"
"Do you want to restart NVDA now?"
